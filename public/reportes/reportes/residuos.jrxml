and b.cia_id>=$P{cia_f} and b.cia_id<=$P{cia_t}
and b.residuo>=$P{residuo_f} and b.residuo<=$P{residuo_t}
and b.responsable_id>=$P{responsable_f} and b.responsable_id<=$P{responsable_t}
